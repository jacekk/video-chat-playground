{
  "name": "video-chat-playground",
  "version": "0.1.0",
  "main": "README.md",
  "author": "JacekK",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:client": "rimraf ./dist && parcel build client/index.html --no-source-maps",
    "clear": "rimraf ./dist ./.cache",
    "serve": "serve ./dist --single --listen 9000",
    "start:client": "parcel client/index.html",
    "start:server": "nodemon --watch ./server ./server/index.js localhost 9876"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "dependencies": {
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@react-hook/window-size": "^1.0.13",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.4",
    "pem": "^1.14.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-use-dimensions": "^1.2.1",
    "react-use-websocket": "^1.3.4",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3",
    "serve": "^11.3.0",
    "uuid": "^7.0.2",
    "websocket": "^1.0.31"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  }
}
